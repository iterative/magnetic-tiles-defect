{
  "name": "magnetic-tiles-defect",
  "features": {
    "ghcr.io/devcontainers/features/nvidia-cuda:1": {
      "installCudnn": true
    }
  },
  "extensions": [
    "Iterative.dvc",
    "ms-python.python",
    "redhat.vscode-yaml"
  ],
  "postCreateCommand": [
    "bash .devcontainer/nvtop_install.sh;",
    "pip3 install --upgrade pip;",
    "pip3 install --user -r requirements.txt;",
    "python3 -m dvc pull"
  ]
}